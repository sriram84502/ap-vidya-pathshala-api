{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/login/(.*)",
      "dest": "src/app.js"
    },
    {
      "src": "/api/health",
      "dest": "src/app.js"
    },
    {
      "src": "/api-docs",
      "dest": "src/app.js"
    },
    {
      "src": "/",
      "dest": "src/app.js"
    }
  ]
} 